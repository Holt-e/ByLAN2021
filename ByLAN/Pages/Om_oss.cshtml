@page
@model ByLAN.Pages.Om_oss

@{
    ViewData["Title"] = "Om oss";
}
<h1>@ViewData["Title"]</h1>

<!DOCTYPE html>

<html>
<head>
    <title></title>
</head>
<body>
<div>
    <div class="containerSign">
    
        <form id="signup">
    
            <div class="headerSign">
            
                <h3>Sign Up</h3>
                
                <p>You want to fill out this form</p>
                
            </div>
            
            <div class="sep"></div>
    
            <div class="inputs">
            
                <input type="email" placeholder="e-mail" autofocus />
            
                <input type="password" placeholder="Password" />
                
                <div class="checkboxy">
                    <input name="cecky" id="checky" value="1" type="checkbox" /><label class="terms">I accept the terms of use</label>
                </div>
                
                <a id="submit" href="#">SIGN UP FOR INVITE NOW</a>
            
            </div>
    
        </form>
    
    </div>
</div>
</body>
</html>